                </svg>
                <h2>Álbuns em Destaque</h2>

            </div>
            <div class="grid">
                <%albuns.forEach(album=>{%>
                    <div class="album-card">
                        <div class="album-cover">
                            <img src="<%=album.foto%>"
                                alt="<%=album.titulo%>">
                            <div class="album-overlay">
                                <svg fill="none" stroke="currentColor" viewBox="0 0 24 24">
